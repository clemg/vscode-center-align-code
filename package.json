{
  "name": "center-align-code",
  "displayName": "Center Align Code",
  "description": "Center align your code in Visual Studio Code",
  "version": "1.0.2",
  "publisher": "clemg",
  "main": "./out/extension.js",
  "categories": [
    "Other",
    "Formatters"
  ],
  "keywords": [
    "format center align"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clemg/vscode-center-align-code"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "extension.centerText",
        "title": "Center align code"
      },
      {
        "command": "extension.toggleAutoCenter",
        "title": "Enable auto code centering"
      }
    ],
    "keybindings": [
      {
        "command": "extension.centerText",
        "key": "cmd+e"
      },
      {
        "command": "extension.toggleAutoCenter",
        "key": "cmd+alt+e"
      },
      {
        "command": "extension.centerText",
        "key": "ctrl+e"
      },
      {
        "command": "extension.toggleAutoCenter",
        "key": "ctrl+alt+e"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0"
  }
}
